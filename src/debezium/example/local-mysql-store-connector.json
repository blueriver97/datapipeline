  "database.include.list": "store",
  "table.include.list": "store.TB_UPPER,store.tb_lower,store.TB_COMPOSITE_KEY",
  "topic.prefix": "local",
  "schema.history.internal.kafka.bootstrap.servers": "kafka.svc.internal:9092",
  "schema.history.internal.kafka.topic": "schemahistory.local.store",
  "schema.history.internal.store.only.captured.tables.ddl": "false",
  "schema.history.internal.kafka.recovery.poll.interval.ms": "1000",
  "key.converter": "io.confluent.connect.avro.AvroConverter",
  "key.converter.schema.registry.url": "http://schema-registry.svc.internal:8081/",
  "value.converter": "io.confluent.connect.avro.AvroConverter",

{
  "name": "s3sink-local-connector",
  "connector.class": "io.confluent.connect.s3.S3SinkConnector",
  "tasks.max": "1",
  "topics": "local.store.tb_lower,local.store.TB_UPPER,local.store.TB_COMPOSITE_KEY",
  "topics.dir": "data/topics",
  "format.class": "io.confluent.connect.s3.format.parquet.ParquetFormat",
  "flush.size": "1000",
  "storage.class": "io.confluent.connect.s3.storage.S3Storage",
  "s3.compression.type": "none",
  "behavior.on.null.values": "ignore",
  "timestamp.extractor": "Record",
  "locale": "ko_KR",
  "timezone": "Asia/Seoul",
  "s3.bucket.name": "blueriver-datalake",
  "s3.region": "ap-northeast-2",
  "aws.access.key.id": "",
  "aws.secret.access.key": "",
  "partitioner.class": "io.confluent.connect.storage.partitioner.DailyPartitioner",
  "rotate.schedule.interval.ms": "60000",
  "key.converter": "io.confluent.connect.avro.AvroConverter",
  "value.converter": "io.confluent.connect.avro.AvroConverter",
  "key.converter.schema.registry.url": "http://schema-registry.svc.internal:8081",
  "value.converter.schema.registry.url": "http://schema-registry.svc.internal:8081",
  "schema.compatibility": "NONE",
  "header.converter": "org.apache.kafka.connect.storage.StringConverter"
}
